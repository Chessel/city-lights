<script src="js/jquery.als-1.7.min.js"></script>

      	<script>
      	$)(document).ready(function(){
      		$(#horizontalList).als();
      	});

      	$("#horizontalList").als({
      		visible_items: 4,      	
      		scrolling_items: 1,      	
      		orientation:"vertical",      	
      		circular:"yes",      	
      		auto:scroll:"yes",      
      		interval: 4000,
      		direction:"down",      	
      		start_from: 3,
      		speed: 400,
      		easing:"linear"
      	});
      	</script>


